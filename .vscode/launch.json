	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/push_swap",
			"args": ["59 98 21 48 49 8 30 51 43 27 72 71 80 68 61 46 39 34 19 6 54 9 77 32 79 78 14 25 7 88 73 66 58 74 4 36 83 40 82 63 91 35 12 11 20 23 10 57 52 81 22 87 18 56 90 94 95 89 47 65 76 33 41 67 26 69 29 42 17 37 85 75 96 64 93 38 24 5 99 86 45 44 97 1 2 53 31 50 84 16 15 28 13 3 100 60 70 62 92 55"],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		}

	]
}
