	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/push_swap",
			"args": ["-40 -13 17 -31 18 50 36 -10 32 1 -12 3 41 -4 -28 -47 -2 -30 -42 -16 39 28 4 48 -14 14 40 12 -22 10 -41 35 47 -6 23 -39 7 44 -35 -37 -48 -36 -27 21 30 -49 49 26 38 -9 -19 33 6 22 -5 -15 19 27 15 -46 -26 37 -8 -29 31 -3 45 -43 -24 11 -23 -32 8 -45 -17 -44 43 13 34 -21 0 42 -7 9 46 2 29 -38 -1 -11 -34 20 5 -33 -20 -25 24 -18 25 16"],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		}

	]
}
