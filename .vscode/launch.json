	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/push_swap",
			"args": ["69 10 99 90 2 62 56 49 95 77 41 28 46 61 86 60 39 34 63 75 7 23 14 55 9 36 53 83 48 37 67 29 15 4 11 73 26 66 3 87 44 31 92 42 50 85 58 100 21 45 93 38 8 13 94 27 64 79 16 17 84 18 52 78 32 96 24 25 1 76 98 54 12 33 5 72 6 91 47 30 71 22 88 89 74 70 59 68 65 19 40 97 81 57 43 82 35 20 80 51"],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		}

	]
}
